import asyncio
import random
import discord

BOT_PREFIX = ("?", "!")
TOKEN = 'INSERT DISCORD TOKEN'

@client.command()
async def coinflip(ctx):
    possible_responses = [
        'Tails' ,
        'Heads' ,
    ]
    await ctx.send(random.choice(possible_responses))
   
